\mode<presentation>

\definecolor{darkred}{rgb}{0.65,0.15,0.25}
\definecolor{lightgray}{gray}{0.90}
\definecolor{mblue}{RGB}{94,135,173}
\colorlet{mred}{red!80!black}

% INNER
\setbeamercolor{frametitle}{fg=mblue!75!black, bg=white}
\setbeamercolor{sur:head}{bg=mblue!75!black,fg=white}
\setbeamercolor{sur:bloc}{bg=black!5,fg=black}

\setbeamercolor{title}{fg = mblue!75!black}
\setbeamercolor{part title}{fg = mblue!75!black}

% In case you choose to display the Table of Contents, or the Outline slide.
\setbeamercolor{section in toc}{fg = mblue!75!black}
%\setbeamercolor{section in toc shaded}{fg = Ftitle}

% The colour of all the items, subitems and and subsubitems are set to black.
\setbeamercolor{item}{fg = mblue!75!black}

\setbeamercolor{block title}{fg = mblue!75!black, bg = white}
\setbeamercolor{block body}{bg = white}

%\setbeamercolor{block title alerted}{fg = DarkOrange, bg = NavajoWhite}
%\setbeamercolor{block body alerted}{bg = NavajoWhite}
%burlywood1
\setbeamercolor{normal text}{fg = black}
\setbeamercolor{alerted text}{fg = darkred}

\setbeamercolor{section in head/foot}{fg=mblue!75!white,bg=mblue!75!black} 
\setbeamertemplate{section in head/foot shaded}[default][10]

\setbeamercolor{caption name}{fg = darkred}

\setbeamercolor{bibliography entry title}{fg = mblue!75!black, bg = white}
\setbeamercolor{bibliography entry author}{fg = mblue!75!black, bg = white}

\mode
<all>
